# Project (>> Don't edit it. <<)
PROJECT_NAME={{ project_name }}

# Environment
ENV=test
DEBUG=True

# MySQL database (If False development will use sqlite3 database.)
MYSQL=True
MYSQL_NAME={{ project_name }}
MYSQL_USER=root
MYSQL_PASSWORD=root
MYSQL_HOST=mysql
MYSQL_PORT=3306

# Admin
ADMIN_NAME=django
ADMIN_EMAIL=me@django.com

# Email
MAILGUN_API_KEY=
MAILGUN_SENDER_DOMAIN=
SERVER_EMAIL=
EMAIL_BACKEND=anymail.backends.mailgun.EmailBackend
EMAIL_SUBJECT_PREFIX=[{{ project_name }}]

# Compress
COMPRESS_ENABLED=True

# Celery
BROKER_URL=amqp://guest@rabbitmq:5672//
CELERY_RESULT_BACKEND=db+mysql://root:root@mysql/celery
